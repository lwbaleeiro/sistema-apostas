api:
  security:
    token:
      secret:  ${JWT_SECRET:your-secret-key}

spring:
  application:
    name: Game Odds

  datasource:
    url: jdbc:postgresql://localhost:5432/gameodds_db
    username: your-user-name
    password: your-password
    driver-class-name: org.postgresql.Driver

  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true
      